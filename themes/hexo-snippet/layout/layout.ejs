<%- partial('_partial/head') %>
<%- partial('_partial/_head-sections/IE',null,{cache: !config.relative_link}) %>
<body>
    <%- partial('_partial/header') %>
    <%- partial('_partial/nav') %>
    <section class="content-wrap">
        <div class="container">
            <div class="row">
                <main class="col-md-8 main-content <%if(is_post()){%>m-post<%}%>">
                    <%- body %>
                </main>
                <% if(theme.toc && is_post()){ %>
                    <%- partial('_partial/toc', null, {cache:false}) %>
                <% } else { %>
                    <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
                <% } %>
            </div>
        </div>
    </section>
    <%- partial('_partial/footer') %>
    <%- partial('_partial/copyright') %>
	<script src="/js/jquery.min.js"></script>
    <!-- 页面点击小红心 
    <script type="text/javascript" src="/js/love.js"></script>
	-->
    <script type="text/javascript" src="/js/value_of_core.js"></script>
	<!--动态线条背景-->
	<script type="text/javascript"
		color="220,220,220" opacity='0.7' zIndex="-2" count="200" src="//cdn.bootcss.com/canvas-nest.js/1.0.0/canvas-nest.min.js">
	</script>

</body>
</html>
